	<queryString>
		<![CDATA[SELECT 
    a.nama_usaha,
    a.no_telp_usaha,
    a.alamat,
    pi.kode_penjualan,
    pi.kode_barang,
    b.nama_barang,
    b.harga_jual,
    pi.jumlah_terjual,
    pi.total_harga,
    pi.jumlah_harga,
    pi.total_bayar,
    pi.kembali,
    ak.nama
FROM 
    print_invoice pi
JOIN 
    about a ON pi.id_about = a.id_about
JOIN 
    barang b ON pi.kode_barang = b.kode_barang
JOIN 
    penjualan p ON pi.kode_penjualan = p.kode_penjualan
JOIN 
    akun_karyawan ak ON p.kode_user = ak.kode_user;]]>
	</queryString>
